{
  "name": "@sendaifun/sonic-agent-kit",
  "version": "1.0.1",
  "description": "connect any ai agents to sonic protocols",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "docs": "typedoc src --out docs",
    "test": "tsx test/index.ts",
    "test:vercel-ai": "tsx test/agent_sdks/vercel_ai.ts",
    "generate": "tsx src/utils/keypair.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "tool-summary": "tsx src/utils/analyzeTools.ts",
    "tool-summary:langchain": "tsx src/utils/analyzeTools.ts --langchain",
    "check-tool-names:langchain": "tsx scripts/check-langchain-tool-duplicates.ts"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "solana",
    "agent",
    "ai",
    "solana agent kit",
    "sendai"
  ],
  "author": "sendaifun",
  "license": "Apache-2.0",
  "dependencies": {
    "@ai-sdk/openai": "^1.0.11",
    "@langchain/core": "^0.3.26",
    "@langchain/groq": "^0.1.2",
    "@langchain/langgraph": "^0.2.36",
    "@langchain/openai": "^0.3.16",
    "@metaplex-foundation/digital-asset-standard-api": "^1.0.4",
    "@metaplex-foundation/mpl-core": "^1.1.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.3.0",
    "@metaplex-foundation/mpl-toolbox": "^0.9.4",
    "@metaplex-foundation/umi": "^0.9.2",
    "@metaplex-foundation/umi-bundle-defaults": "^0.9.2",
    "@metaplex-foundation/umi-options": "^1.0.0",
    "@metaplex-foundation/umi-uploader-irys": "^1.0.0",
    "@metaplex-foundation/umi-web3js-adapters": "^0.9.2",
    "@solana/spl-token": "^0.4.9",
    "@solana/spl-token-metadata": "^0.1.6",
    "@solana/web3.js": "^1.98.0",
    "ai": "^4.0.22",
    "bn.js": "^5.2.1",
    "bs58": "^6.0.0",
    "chai": "^5.1.2",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.7",
    "langchain": "^0.3.8",
    "openai": "^4.77.0",
    "tiktoken": "^1.0.18",
    "typedoc": "^0.27.6",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.6",
    "@types/chai": "^5.0.1",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.15.3"
}